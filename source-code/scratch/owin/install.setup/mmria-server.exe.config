    <add key="timer_password" value="couchdb_admin_password" />

    <add key="cron_schedule" value="0 */1 * * * ?" />
    <!--
    
      Quartz.net cron expression examples
    1. Seconds
    2. Minutes
    3. Hours
    4. Day-of-Month
    5. Month
    6. Day-of-Week
    7. Year (optional field)
    
    every minute = "0 */1 * * * ?"
    every 5 minutes = "0 */5 * * * ?"
    every 30 minutes = "0 */30 * * * ?"
    every hour = "0 0 */1 * * ?"
    every 12 hours = "0 0 */12 * * ?"
 -->
  </appSettings>
