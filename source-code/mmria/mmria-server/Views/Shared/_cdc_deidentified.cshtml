  <style>
  .grid {
    display: grid;
    grid-template-rows: 155px 1fr 155px;
  }

  table .grid {
    display: block
  }

  #header 
  {
    background-color: #FFFFFF;
    display: grid;
    grid-template-columns: 1f  1f 1f;
    grid-template-rows: 1f  1f;
    border: 0px;
    height:  70px;
    padding: 20px 0;
    transition: all 0.3s;
    color: #FFFFFF;
    position: fixed; /* Set the navbar to fixed position */
    width: 100%; /* Full width */
    height: 200px;
    z-index: 10;
    top: 0;
    
  }        

  .header-top-left
  {
    grid-row 1;
    grid-column: 1;
  }

  .header-top-right
  {
    grid-row: 1;
    grid-column: 3;
  }

  .header-second-row 
  {
    grid-row: 2;
    grid-column: 1;
  }


  #content
  {
    /*height:  666px;*/
    align-content: center;
    margin: 20px;
    margin-top: 50px;
    overflow-y: auto;

  }

  .center 
  {
    display: block;
    margin-left: auto;
    margin-right: auto;
    /*width: 50%;*/
  
  }

  #footer 
  {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: red;
    color: white;
    /*text-align: center;*/
  }
</style>
