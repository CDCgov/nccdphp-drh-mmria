@section Scripts
{
  <script>
    $('body').on('click', '#content-link', function(event) {
      var tar = $('h1[tabindex]');
      event.preventDefault();

      tar.focus();
    });


    $('#nav-link').show();
    $('body').on('click', '#nav-link', function() {
      var tar = $('#navigation[tabindex]');
      event.preventDefault();

      tar.focus();
    });
  </script>
}

